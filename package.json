{
  "name": "yz-xiuxian",
  "version": "1.0.0-rc.3",
  "description": "喵崽修仙插件",
  "main": "main.js",
  "type": "module",
  "author": "ningmengchongshui",
  "license": "MIT",
  "scripts": {
    "app": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/main.js",
    "dev": "nodemon",
    "img": "node --no-warnings=ExperimentalWarning --loader ts-node/esm routes.server.ts",
    "css": "tailwindcss -i ./src/input.css -o ./public/output.css",
    "css:m": "tailwindcss -i ./src/input.css -o ./public/output.css -m",
    "css:w": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "prepare": "husky",
    "format": "npx prettier --write .",
    "build": "npm run css:m && rollup --config rollup.config.js"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "puppeteer": "^22.13.1",
    "react": "^18.3.1",
    "react-puppeteer": "^1.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.3.1",
    "@types/progress": "^2.0.5",
    "@types/react": "18.2.73",
    "dotenv": "^16.4.1",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "2.8.7",
    "rollup": "^3.28.1",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "yunzai": "^1.0.1-rc.1"
  },
  "files": [
    "public",
    "public/output.css",
    "resources",
    "lib"
  ],
  "types": "lib/types",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "keywords": [
    "yunzai"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
